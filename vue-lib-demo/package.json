{
  "name": "vue-lib-demo",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vue-tsc -b && vite build --mode lib",
    "preview": "vite preview"
  },
  "main": "./dist/vue-lib.umd.js",
  "module": "./dist/vue-lib.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-lib.es.js",
      "require": "./dist/vue-lib.umd.js"
    },
    "./dist/vue-lib.css": "./dist/vue-lib.css"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "vue": "3.5.13",
    "vue-facing-decorator": "^4.0.1",
    "vue-lib": "file:../vue-lib"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/tsconfig": "0.7.0",
    "sass": "1.69.5",
    "sass-embedded": "1.89.2",
    "syncpack": "13.0.4",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vue-tsc": "2.2.8",
    "vue-class-component": "8.0.0-rc.1",
    "vue-property-decorator": "10.0.0-rc.3"
  },
  "typings": {
    "vue-lib": "./node-modules/vue-lib/dist/vue-lib.d.ts"
  }
}
